{
  "name": "co-bridge",
  "displayName": "CoBridge",
  "description": "Seamlessly sync your web AI conversations to your local IDE.",
  "version": "0.0.3",
  "publisher": "windfall",
  "repository": {
    "type": "git",
    "url": "https://github.com/Winddfall/CoBridge-vscode-extension.git"
  },
  "license": "MIT",
  "icon": "public/icon.png",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ai-context-sync.startServer",
        "title": "AI Sync: Start Receiving Server"
      },
      {
        "command": "ai-context-sync.stopServer",
        "title": "AI Sync: Stop Receiving Server"
      }
    ],
    "configuration": {
      "title": "CoBridge",
      "properties": {
        "AIContextSync.port": {
          "type": "number",
          "default": 3030,
          "description": "Port to listen for browser sync data."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.50.0",
    "typescript": "^4.9.5"
  }
}
